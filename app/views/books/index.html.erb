
<h1>Book Index</h1>

<table>

<tr>
  <th>Title</th>
  <th>Author</th>
  <th>Year Published</th>
  <th>Summary</th>
  <th>Actions</th>
</tr>

<% @books.each do |book| %>
<tr>
<td><%= book.title %></td>
<td><%= book.my_author.first_name %> <%= book.my_author.last_name %></td>
<td><%= book.year_published %></td>
<td><%= book.summary %></td>
<td>[...]</td>
</tr>
<% end %>

</table>

<p>
  <%= link_to "Create Book", new_book_path %>
</p>
